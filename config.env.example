# Database Configuration
MYSQL_ROOT_PASSWORD=your_secure_root_password
MYSQL_USER=data_platform_user
MYSQL_PASSWORD=your_secure_password

# MinIO Configuration
MINIO_ROOT_USER=admin
MINIO_ROOT_PASSWORD=your_secure_minio_password

# InfluxDB Configuration
INFLUXDB_USERNAME=admin
INFLUXDB_PASSWORD=your_secure_influxdb_password

# Airflow Configuration
AIRFLOW_FERNET_KEY=your_generated_fernet_key

# Grafana Configuration
GRAFANA_PASSWORD=your_secure_grafana_password

# Azure AD Configuration
AZURE_AD_TENANT_ID=your_tenant_id
AZURE_AD_CLIENT_ID=your_client_id
AZURE_AD_CLIENT_SECRET=your_client_secret

# Microsoft Fabric Configuration
MICROSOFT_FABRIC_WORKSPACE_ID=your_workspace_id
MICROSOFT_FABRIC_CLIENT_ID=your_fabric_client_id
MICROSOFT_FABRIC_CLIENT_SECRET=your_fabric_client_secret

# Google Analytics Configuration
GOOGLE_ANALYTICS_PROPERTY_ID=your_ga_property_id
GOOGLE_ANALYTICS_VIEW_ID=your_ga_view_id

# Smartlook Configuration
SMARTLOOK_PROJECT_ID=your_smartlook_project_id
SMARTLOOK_API_KEY=your_smartlook_api_key

# Kong API Gateway Configuration
KONG_ADMIN_TOKEN=your_kong_admin_token

# Open Metadata Configuration
OPENMETADATA_ADMIN_EMAIL=admin@yourdomain.com
OPENMETADATA_ADMIN_PASSWORD=your_secure_admin_password

# Monitoring Configuration
PROMETHEUS_RETENTION_DAYS=30
GRAFANA_ADMIN_PASSWORD=your_secure_grafana_admin_password

# Network Configuration
EXTERNAL_DOMAIN=your-domain.com
INTERNAL_DOMAIN=internal.your-domain.com

# SSL Configuration
SSL_CERT_PATH=/path/to/ssl/certificate
SSL_KEY_PATH=/path/to/ssl/private/key

# Backup Configuration
BACKUP_RETENTION_DAYS=90
BACKUP_SCHEDULE=0 2 * * *

# Security Configuration
JWT_SECRET_KEY=your_jwt_secret_key
ENCRYPTION_KEY=your_encryption_key

# Performance Configuration
MAX_CONNECTIONS=100
CONNECTION_TIMEOUT=30
QUERY_TIMEOUT=300

# Logging Configuration
LOG_LEVEL=INFO
LOG_RETENTION_DAYS=30
